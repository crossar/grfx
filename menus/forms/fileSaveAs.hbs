<form>

	<title>Save File As...</title>
	<div>
		<label for="image-url">Filename</label>
		<input
			type="text" id="filename" name="filename" autocomplete="off"
			autofocus
			value="{{filename}}"
		>
	</div>
	<div class="row"></div>

	<div class="row align-right">
		<input type="button" value="Cancel">
		<input type="submit" value="OK">
	</div>
</form>
